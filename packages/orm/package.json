{
  "name": "ivy-orm",
  "version": "0.0.25",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts --format cjs,esm --dts --minify --external @azure/search-documents",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:types": "tsd --files test/field-builder.test-d.ts",
    "pub:release": "pnpm build && pnpm publish --no-git-checks"
  },
  "keywords": [
    "azure",
    "cognitive",
    "search",
    "ai",
    "orm",
    "typescript",
    "ts"
  ],
  "author": "ihusemann",
  "repository": {
    "type": "git",
    "url": "https://github.com/ihusemann/ivy-orm"
  },
  "license": "MIT",
  "devDependencies": {
    "@azure/search-documents": "^12.0.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "tsd": "^0.31.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^2.1.1"
  }
}
